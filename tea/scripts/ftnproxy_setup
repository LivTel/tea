#!/bin/csh

# setup classpath.

set JBIN = /occ/common/ngat
set EBIN = /occ/tea/javalib

setenv CLASSPATH .

foreach jar ( $EBIN/org_estar_astrometry.jar $EBIN/org_estar_fits.jar $EBIN/org_estar_io.jar $EBIN/org_estar_io_test.jar $EBIN/org_estar_rtml.jar $EBIN/org_estar_rtml_test.jar $EBIN/jfits-0.91.jar $JBIN/ngat_util.jar $JBIN/ngat_util_logging.jar $JBIN/ngat_net.jar $JBIN/ngat_net_camp.jar $JBIN/ngat_astrometry.jar $JBIN/ngat_phase2.jar $JBIN/ngat_message_base.jar $JBIN/ngat_message_oss.jar $JBIN/ngat_message_gui_rcs.jar ${CLASSPATH}:$EBIN/tea.jar )
    echo $CLASSPATH | /bin/grep -q $jar
    if ( $status != 0 ) then
	setenv CLASSPATH ${CLASSPATH}":"${directory}
    endif
end

set LIB = /occ/tea/elib
setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:$LIB

set CLIB = /occ/common/lib
setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:$CLIB
